                                </a>
                            </li>
                        </ul>*@
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("ListSubCategory", "Admin")">
                            <i class="mdi mdi-gauge"></i> Sub Category
                        </a>
                        @*<ul class="nav-dropdown-items">

                            <li class="nav-item">
                                <a href="@Url.Action("ListSubCategory", "Admin")" class="nav-link">
                                    <i class="fa fa-plus" style="font-size:12px"></i> <span>  View </span>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="@Url.Action("AddSubCategory", "Admin")" class="nav-link">
                                    <i class="fa fa-plus" style="font-size:12px"></i> <span>  Add </span>
                                </a>
                            </li>

                            <li class="nav-item">
                                <a href="@Url.Action("EditSubCategory", "Admin")" class="nav-link">
                                    <i class="fa fa-plus" style="font-size:12px"></i> <span>  Edit </span>
                                </a>
                            </li>
                        </ul>*@
                    </li>
                    <li class="nav-item nav-dropdown">
                        <a class="nav-link nav-dropdown-toggle" href="#">
                            <i class="fa fa-gear fa-spin"></i> Account Settings
                        </a>

                        <ul class="nav-dropdown-items">
                            <li class="nav-item">
                                <a href="@Url.Action("ProfileView", "Admin")" class="nav-link">
                                    <i class="fa fa-plus" style="font-size:12px"></i> <span>  Profile </span>

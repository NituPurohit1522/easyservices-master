<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="Admin, Dashboard, Bootstrap" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>@ViewBag.Title</title>

    <link rel="apple-touch-icon" sizes="180x180" href="~/octadmin/img/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/octadmin/img/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/octadmin/img/favicon/favicon-16x16.png">
    <link rel="manifest" href="~/octadmin/img/favicon/manifest.json">
    <link rel="mask-icon" href="~/octadmin/img/favicon/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="theme-color" content="#ffffff">

    <!-- fonts -->
    <link rel="stylesheet" href="~/octadmin/fonts/md-fonts/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/octadmin/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!-- animate css -->
    <link rel="stylesheet" href="~/octadmin/libs/animate.css/animate.min.css">

    <!-- jquery-loading -->
    <link rel="stylesheet" href="~/octadmin/libs/jquery-loading/dist/jquery.loading.min.css">
    <!-- octadmin main style -->
    <link id="pageStyle" rel="stylesheet" href="~/octadmin/css/style.css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    @RenderBody()

    <div id="copyright">
        <script language="JavaScript" type="text/javascript">document.write((new Date()).getFullYear());</script>
        
    </div>

    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        $(function () {

        });
    </script>
    <!-- Bootstrap and necessary plugins -->
    <script src="~/octadmin/libs/jquery/dist/jquery.min.js"></script>
    <script src="~/octadmin/libs/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/octadmin/libs/bootstrap/bootstrap.min.js"></script>
    <script src="~/octadmin/libs/PACE/pace.min.js"></script>
    <script src="~/octadmin/libs/chart.js/dist/Chart.min.js"></script>
    <script src="~/octadmin/libs/nicescroll/jquery.nicescroll.min.js"></script>

    <script src="~/octadmin/libs/jquery-knob/dist/jquery.knob.min.js"></script>

    <!-- jquery-loading -->
    <script src="~/octadmin/libs/jquery-loading/dist/jquery.loading.min.js"></script>
    <!-- octadmin Main Script -->
    <script src="~/octadmin/js/app.js"></script>
</body>
</html>
